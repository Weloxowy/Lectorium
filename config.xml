<?xml version="1.0" encoding="UTF-8"?>
<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  <jar>..\out\artifacts\Lectorium_jar\Lectorium.jar</jar>
  <outfile>Lectorium.exe</outfile>
  <errTitle>Error</errTitle>
  <cmdLine>-jar out/artifacts/Lectorium_jar/Lectorium.jar</cmdLine>
  <chdir>.</chdir>
  <priority>normal</priority>
  <downloadUrl>http://java.com/download</downloadUrl>
  <stayAlive>false</stayAlive>
  <restartOnCrash>false</restartOnCrash>
  <manifest>                       </manifest>
  <icon>..\src\main\resources\res\logo\Lectorium_logo.png</icon>
  <classPath>
    <mainClass>com.example.Run</mainClass>
    <cp>..\out\artifacts\Lectorium_jar\Lectorium.jar</cp>
    <cp>..\database.db</cp>
  </classPath>
  <jre>
    <path>%JAVA_HOME%</path>
    <bundledJre64Bit>false</bundledJre64Bit>
    <bundledJreAsFallback>false</bundledJreAsFallback>
    <minVersion>1.7.0</minVersion>
    <maxVersion></maxVersion>
    <jdkPreference>preferJre</jdkPreference>
    <runtimeBits>64/32</runtimeBits>
  </jre>
